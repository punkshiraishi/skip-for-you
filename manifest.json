  "manifest_version": 3,
  "icons": {
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ]
    }
  ]
}